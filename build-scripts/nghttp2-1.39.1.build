#! /bin/bash

# nghttp2 1.39.1
# Source: https://github.com/nghttp2/nghttp2/releases/download/v1.39.1/nghttp2-1.39.1.tar.xz

./configure --prefix=/usr     \
            --disable-static  \
            --enable-lib-only \
            --docdir=/usr/share/doc/nghttp2-1.32.0 $BUILDTRUPLE && 
read -p "Compile?" && make -j2  && 
read -p "Install?" && sudo -S porg -lD "make install"
