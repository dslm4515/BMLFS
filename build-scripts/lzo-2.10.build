#! /bin/bash

# LZO 2.10
# Source: http://www.oberhumer.com/opensource/lzo/download/lzo-2.10.tar.gz

./configure --prefix=/usr                    \
            --enable-shared                  \
            --disable-static                 \
            --docdir=/usr/share/doc/lzo-2.10 $BUILDTRUPLE
read -p "Compile?" && make -j2 &&
read -p "Install?" && sudo -S porg -lD "make install"
