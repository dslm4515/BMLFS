#! /bin/bash

# Libgpg-error 1.36
# Source: https://www.gnupg.org/ftp/gcrypt/libgpg-error/libgpg-error-1.36.tar.bz2

patch -Np0 -i ../patches/libgpg-error-1.36-void-001-awk.patch &&

./configure --prefix=/usr $BUILDTRUPLE &&
read -p "Compile?" && make -j2 && 
read -p "Install?" && sudo -S porg -lD "make install "
