#! /bin/bash

# Harfbuzz 2.5.3
# Source: https://www.freedesktop.org/software/harfbuzz/release/harfbuzz-2.5.3.tar.xz

./configure --prefix=/usr \
            --with-gobject \
            --with-graphite2 $BUILDTRUPLE &&
read -p "Compile? " && make -j2 && 
read -p "Install? " && sudo -S porg -lD "make install"
