+++ b/vendor/rustix-0.38.2/src/backend/libc/termios/syscalls.rs
@@ -32,7 +32,7 @@ pub(crate) fn tcgetattr(fd: BorrowedFd<'_>) -> io::Result<Termios> {
 
         ret(c::ioctl(
             borrowed_fd(fd),
-            c::TCGETS2.into(),
+            c::TCGETS2 as _,
             termios2.as_mut_ptr(),
         ))?;
 
